{
  "version": 2,
  "builds": [
    {
      "src": "magicmushroom.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)\\.(css|js|png|jpg|jpeg|gif|svg|ico|webp|ttf|woff|woff2|eot|otf)$",
      "dest": "/$1.$2"
    },
    {
      "src": "/(.*)",
      "dest": "/magicmushroom.html"
    }
  ]
}
